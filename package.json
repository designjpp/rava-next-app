{
  "name": "rava-app",
  "description": "Rava Cycle institucional Site",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "next",
    "starter",
    "react",
    "typescript",
    "next-starter",
    "nextjs",
    "ueno"
  ],
  "maintainers": [
    {
      "name": "Eder Oliveira",
      "email": "design@grupojpp.com.br",
      "web": "htts://grupojpp.com.br"
    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "develop": "ts-node-dev --transpileOnly --project src/tsconfig.json --compiler-options '{\"module\":\"commonjs\"}' --no-notify --ignore-watch .next src/server",
    "building": "next build src && tsc --project src/tsconfig.json",
    "starting": "cross-env NODE_ENV=production next start build",
    "export": "next export -o ./dist src",
    "lint": "npm run prettier && npm run tslint && npm run stylelint",
    "tslint": "tslint --fix 'src/**/*.{ts,tsx}' --project src/tsconfig.json",
    "stylelint": "stylelint 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --list-different '**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tslint --fix"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ]
  },
  "dependencies": {
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "algoliasearch": "^4.5.1",
    "axios": "^0.20.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "lodash": "4.17.20",
    "next": "^9.5.3",
    "next-compose-plugins": "2.2.0",
    "next-instantsearch": "^0.3.15",
    "next-optimized-images": "^2.6.2",
    "next-pwa": "^3.1.5",
    "next-videos": "1.4.0",
    "polished": "3.6.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-instantsearch-dom": "^6.7.0",
    "styled-components": "5.2.0",
    "swiper": "^6.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@types/express": "4.17.8",
    "@types/lodash": "4.14.161",
    "@types/next": "9.0.0",
    "@types/node": "^14.11.2",
    "@types/react": "16.9.50",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-styled-components": "1.11.1",
    "cross-env": "7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "stylelint": "13.7.2",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-react-loader": "0.4.6",
    "svgo": "^1.3.2",
    "ts-node-dev": "1.0.0-pre.63",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.2.0",
    "typescript": "4.0.3"
  }
}
